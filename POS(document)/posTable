--  DDL for Table BILL
--------------------------------------------------------

  CREATE TABLE "CHOI"."BILL" 
   (	"CUST_NO" VARCHAR2(9 BYTE), 
	"STL_ACT" VARCHAR2(20 BYTE), 
	"BNK_CD" VARCHAR2(3 BYTE), 
	"DPS_NM" VARCHAR2(30 BYTE), 
	"STL_MTD" VARCHAR2(1 BYTE), 
	"STL_DD" VARCHAR2(2 BYTE), 
	"PRCS_CLAS" VARCHAR2(1 BYTE), 
	"STMT_SND_MTD" VARCHAR2(1 BYTE), 
	"STMT_DENI_CLAS" VARCHAR2(1 BYTE), 
	"BILL_ZIP" VARCHAR2(5 BYTE), 
	"BILL_ADR1" VARCHAR2(60 BYTE), 
	"BILL_ADR2" VARCHAR2(60 BYTE), 
	"EMAIL_ADR" VARCHAR2(60 BYTE), 
	"LST_OPR_TM" VARCHAR2(6 BYTE), 
	"LST_OPR_D" VARCHAR2(8 BYTE), 
	"LST_OPRT_EMPNO" VARCHAR2(8 BYTE)
   ) SEGMENT CREATION IMMEDIATE 
  PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 NOCOMPRESS LOGGING
  STORAGE(INITIAL 65536 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645
  PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1 BUFFER_POOL DEFAULT FLASH_CACHE DEFAULT CELL_FLASH_CACHE DEFAULT)
  TABLESPACE "SYSTEM" ;

   COMMENT ON COLUMN "CHOI"."BILL"."CUST_NO" IS '고객번호';
   COMMENT ON COLUMN "CHOI"."BILL"."STL_ACT" IS '결제계좌';
   COMMENT ON COLUMN "CHOI"."BILL"."BNK_CD" IS '결제은행(은행코드 참조)';
   COMMENT ON COLUMN "CHOI"."BILL"."DPS_NM" IS '예금주명';
   COMMENT ON COLUMN "CHOI"."BILL"."STL_MTD" IS '결제방법';
   COMMENT ON COLUMN "CHOI"."BILL"."STL_DD" IS '결제일자';
   COMMENT ON COLUMN "CHOI"."BILL"."PRCS_CLAS" IS '계좌확인유무, 계좌확인유무 1.확인 2.미확인';
   COMMENT ON COLUMN "CHOI"."BILL"."STMT_SND_MTD" IS '청구서발송방법(1:우편 2:E-MAIL 3:청구서사절)';
   COMMENT ON COLUMN "CHOI"."BILL"."STMT_DENI_CLAS" IS '청구서사절구분(1:청구서 사절)';
   COMMENT ON COLUMN "CHOI"."BILL"."BILL_ZIP" IS '청구지우편번호';
   COMMENT ON COLUMN "CHOI"."BILL"."BILL_ADR1" IS '청구지주소1';
   COMMENT ON COLUMN "CHOI"."BILL"."BILL_ADR2" IS '청구지주소2';
   COMMENT ON COLUMN "CHOI"."BILL"."EMAIL_ADR" IS 'EMAIL주소';
   COMMENT ON COLUMN "CHOI"."BILL"."LST_OPR_TM" IS '최종작업시간';
   COMMENT ON COLUMN "CHOI"."BILL"."LST_OPR_D" IS '최종작업일자';
   COMMENT ON COLUMN "CHOI"."BILL"."LST_OPRT_EMPNO" IS '최종작업자사번';
